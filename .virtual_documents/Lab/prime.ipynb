def is_prime(n) -> bool:
    if n <= 0:
        return False
    if n == 1:
        return True
    for num in range(2, n):
        if n % num == 0:
            return False
    return True


def generate_primes(limit):
    result = []
    for n in range(2, limit+1):
        if is_prime(n):
            result.append(n)
        
    return result


for num in range(1,26):
    print(num, is_prime(num))


primes = generate_primes(10_000)


