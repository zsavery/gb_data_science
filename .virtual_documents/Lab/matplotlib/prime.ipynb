def is_prime(n) -> bool:
    """
    Checks if the whole number passed into the function is a prime number
    """
    if n <= 0:
        return False
    if n == 1:
        return True
    for num in range(2, n):
        if n % num == 0:
            return False
    return True


def generate_primes(limit):
    """
    Generates a list of prime numbers 
    """
    result = []
    for n in range(limit+1): # [0 .... , limit -1, limit]
        if is_prime(n):
            result.append(n)
        
    return result


primes = generate_primes(100_000)


