import matplotlib.pyplot as plt
# from matplotlib import pyplot as plt
import numpy as np
import math


def plot_prime_spiral(lst):
    fig, ax = plt.subplots(figsize=(8, 8))
    ax.set_aspect('equal')

    max_prime = max(lst)
    max_radius = int(math.sqrt(max_prime)) + 1

    for prime in lst:
        angle = prime * (math.pi / 180)
        radius = math.sqrt(prime)
        x = radius * math.cos(angle)
        y = radius * math.sin(angle)
        ax.plot(x, y, 'go')

    ax.set_xlim(-max_radius, max_radius)
    ax.set_ylim(-max_radius, max_radius)
    ax.set_xlabel('X-Axis')
    ax.set_ylabel('Y-Axis')
    plt.title('Prime Numbers in a Spiral Pattern')
    plt.grid(True)
    plt.show()


plot_prime_spiral(primes)


plot_prime_spiral(primes[:10_000])



